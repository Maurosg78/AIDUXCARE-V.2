+        onDragLeave={() => setIsDragging(false)}
+      >
+        <textarea
+          value={transcript}
+          onChange={(e) => setTranscript(e.target.value)}
+          onPaste={handlePaste}
+          placeholder="Opciones:
+- Escribir directamente aqu√≠
+- Grabar audio con el bot√≥n 'Grabar'
+- Arrastrar archivos aqu√≠
+- Pegar im√°genes con Ctrl+V
+
+Despu√©s presione 'Analizar con IA' ‚Üí"
+          className="w-full h-full p-2 text-sm bg-transparent resize-none focus:outline-none"
+          disabled={isRecording}
+        />
+        
+        {isDragging && (
+          <div className="absolute inset-0 flex items-center justify-center bg-blue-100 bg-opacity-90 rounded-lg">
+            <Upload className="w-8 h-8 text-blue-600" />
+          </div>
+        )}
+        
+        {isProcessing && (
+          <div className="absolute top-1 right-1 text-xs bg-yellow-100 px-2 py-1 rounded">
+            Procesando...
+          </div>
+        )}
+      </div>
+      
+      <input
+        ref={fileInputRef}
+        type="file"
+        multiple
+        accept="image/*,audio/*,.pdf,.txt"
+        onChange={handleFileSelect}
+        className="hidden"
+      />
+      
+      <div className="mt-2 text-xs text-gray-500 text-center">
+        <FileText className="w-3 h-3 inline mr-1" />
+        Soporta: Im√°genes, Audio, PDF, Texto
+      </div>
+    </div>
+  );
+};
