+      
+      const meds = entities
+        .filter(e => e.type === 'medication')
+        .map(e => e.text);
+      
+      const conds = entities
+        .filter(e => e.type === 'condition')
+        .map(e => e.text);
+
+      setPatientInfo(prev => ({
