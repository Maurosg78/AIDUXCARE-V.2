+
+// M√©tricas de √©xito
+const SUCCESS_CRITERIA = {
+  latency: 3000, // <3 segundos
+  successRate: 95, // >95%
+  healthCheck: true
+};
+
+let testResults = {
+  healthCheck: false,
+  cors: false,
+  latency: 0,
+  successRate: 0,
