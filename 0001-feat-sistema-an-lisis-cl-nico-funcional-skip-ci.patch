+            type = 'test';
+            icon = 'üìã';
+          } else {
+            type = 'condition';
+            icon = 'üîç';
+          }
+          
+          if (itemText.length > 2 && !itemText.includes(':')) {
+            entities.push({
+              id: String(idCounter++),
+              text: `${icon} ${itemText}`,
+              type: type,
