+    
+    # Verificar TypeScript
+    if [ ! -f "node_modules/typescript/bin/tsc" ]; then
+        echo -e "${RED}‚ùå TypeScript corrupto detectado${NC}"
+    else
+        echo -e "${GREEN}‚úÖ TypeScript OK${NC}"
+    fi
+else
+    echo -e "${YELLOW}node_modules no existe${NC}"
+fi
+echo ""
+
