+import { useNiagaraProcessor } from "../hooks/useNiagaraProcessor";
+import { useAutoSelection } from "../hooks/useAutoSelection";
+
+interface WorkflowAnalysisTabProps {
+  selectedPatient: any;
+  transcript: string;
+  setTranscript: (text: string) => void;
+  isRecording: boolean;
+  startRecording: () => void;
+  stopRecording: () => void;
+  recordingTime: string;
