{
  "lumbar": {
    "acute": {
      "mandatory": ["SLR", "Neurological screen"],
      "conditional": {
        "if_radicular": ["Slump test", "Femoral stretch test"],
        "if_mechanical": ["Flexion/Extension ROM", "Side glide test"],
        "if_red_flag": ["Bilateral SLR", "Reflexes L3-S1", "Myotomes"]
      }
    },
    "chronic": {
      "mandatory": ["McKenzie assessment", "Core stability test"],
      "conditional": {
        "if_instability": ["Prone instability test", "Aberrant movement"],
        "if_facet": ["Extension/rotation test", "Quadrant test"]
      }
    }
  },
  "cervical": {
    "acute": {
      "mandatory": ["Spurling test", "Cervical ROM"],
      "conditional": {
        "if_radicular": ["ULTT", "Distraction test"],
        "if_mechanical": ["Compression test", "Rotation test"],
        "if_red_flag": ["Vertebral artery test", "Upper motor neuron signs"]
      }
    },
    "chronic": {
      "mandatory": ["Deep neck flexor endurance", "Postural assessment"],
      "conditional": {
        "if_headache": ["Upper cervical flexion", "C0-C1 mobility"],
        "if_whiplash": ["Neck disability index", "Fear avoidance"]
      }
    }
  },
  "shoulder": {
    "acute": {
      "mandatory": ["ROM assessment", "Painful arc test"],
      "conditional": {
        "if_impingement": ["Neer test", "Hawkins-Kennedy test"],
        "if_rotator_cuff": ["Empty can test", "External rotation strength"],
        "if_instability": ["Apprehension test", "Relocation test"]
      }
    }
  }
}
