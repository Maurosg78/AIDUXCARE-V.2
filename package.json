{
  "name": "aiduxcare-v2",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "engines": {
    "node": ">=20.19 <21"
  },
  "scripts": {
    "debug:mcp": "tsx src/core/mcp/debugMCP.ts",
    "dev": "node node_modules/vite/bin/vite.js --port 5174 --host",
    "dev:build": "node node_modules/vite/bin/vite.js build --watch --mode development",
    "dev:serve": "npx serve dist -p 5174",
    "dev:skip-check": "node_modules/.bin/vite",
    "dev:direct": "node_modules/.bin/vite --port 5174 --host",
    "dev:simple": "node_modules/.bin/vite --config vite-simple.config.js --port 5174 --host",
    "dev:minimal": "node node_modules/vite/bin/vite.js --config vite.config.minimal.js --port 5174 --host --force",
    "dev:https": "bash scripts/update-dev-url.sh && node node_modules/vite/bin/vite.js --config vite.config.https.ts --port 5174 --host",
    "dev:tunnel": "bash scripts/start-dev-with-tunnel.sh",
    "setup:tunnel": "bash scripts/setup-cloudflare-tunnel.sh",
    "harness:test": "node scripts/mobile-harness-test.cjs",
    "mobile:preflight": "node scripts/mobile-preflight.cjs",
    "build": "vite build",
    "build:ci": "tsc && vite build --config vite.config.ci.ts",
    "deploy:staging": "npm run build && firebase deploy --only hosting,functions --project aiduxcare-v2-uat-dev",
    "build:migration": "tsc --project tsconfig.migration.json && vite build",
    "lint:prod": "eslint \"src/**/*.{ts,tsx}\" --report-unused-disable-directives --max-warnings 0",
    "lint": "eslint .",
    "lint:fix": "eslint \"src/**/*.{ts,tsx}\" --fix --fix-type layout,suggestion",
    "validate:canonical": "bash scripts/validate-canonical-imports.sh",
    "typecheck": "echo '⏭️ typecheck saltado temporalmente'",
    "preview": "vite preview",
    "test": "vitest",
    "test:unit": "vitest run --passWithNoTests",
    "test:run": "vitest run",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:sms": "tsx scripts/test-twilio-sms.ts",
    "test:consent-flow": "tsx scripts/test-consent-flow.ts",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:command-centre": "playwright test tests/e2e/command-centre.spec.ts",
    "test:e2e:happy-path": "playwright test tests/e2e/command-centre.spec.ts --grep 'happy path'",
    "test:e2e:local": "npm run emulators:start && npm run dev:test & sleep 10 && npm run test:e2e:command-centre && npm run emulators:stop",
    "test:e2e:full": "npm run emulators:start && npm run dev:test & sleep 15 && npm run test:e2e && npm run emulators:stop",
    "auto-save:start": "node scripts/auto-save-workflow.js 300",
    "auto-save:start:fast": "node scripts/auto-save-workflow.js 60",
    "auto-save:start:slow": "node scripts/auto-save-workflow.js 600",
    "auto-save:simple": "bash scripts/auto-save-simple.sh 300",
    "quick-save": "bash scripts/quick-save.sh",
    "emulators:start": "firebase emulators:start --only auth,firestore,functions --project aiduxcare-v2-uat-dev --import=./emulator-data --export-on-exit=./emulator-data",
    "emulators:stop": "pkill -f 'firebase emulators' || true",
    "dev:test": "cp .env.test.local .env.local && vite --port 5174",
    "verify:deploy": "npm run lint --max-warnings 0 && npm run type-check && npm run build && npm run test:e2e:command-centre",
    "verify:functions": "cd functions && npm ci && npm run build && cd ..",
    "verify:all": "npm run verify:deploy && npm run verify:functions",
    "script:createCase": "tsx scripts/createClinicalCase.ts",
    "script:createSecondVisit": "tsx scripts/createSecondVisitAndrea.ts",
    "test:rag": "tsx scripts/test-rag-pubmed.ts",
    "check:env": "node scripts/check-env.cjs",
    "postinstall": "npm run check:env",
    "check:no-soap-logs": "node scripts/check-no-soap-logs.cjs"
  },
  "dependencies": {
    "@firebase/app": "^0.10.14",
    "@firebase/auth": "^1.9.2",
    "@firebase/firestore": "^4.8.2",
    "@firebase/storage": "^0.12.2",
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.9.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.2",
    "@radix-ui/react-label": "^2.1.1",
    "@radix-ui/react-popover": "^1.1.2",
    "@radix-ui/react-select": "^2.1.2",
    "@radix-ui/react-slot": "^1.1.1",
    "@radix-ui/react-tabs": "^1.1.1",
    "@radix-ui/react-toast": "^1.2.2",
    "@supabase/supabase-js": "^2.47.10",
    "@types/bcryptjs": "^2.4.6",
    "@types/react-router-dom": "^5.3.3",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^3.6.0",
    "dotenv": "^16.4.7",
    "firebase": "^11.1.0",
    "jspdf": "^2.5.2",
    "jspdf-autotable": "^3.8.4",
    "lucide-react": "^0.469.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.54.2",
    "react-router-dom": "^6.30.2",
    "tailwind-merge": "^2.6.0",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.49.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.10.2",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "jsdom": "^27.2.0",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vite": "^5.4.20",
    "vitest": "^2.1.8"
  }
}
