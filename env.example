# ========================================
# AiDuxCare V.2 - Configuración de Entornos
# ========================================

# ========================================
# ENTORNO DE DESARROLLO (UAT)
# ========================================
# Copiar a .env para desarrollo
VITE_FIREBASE_PROJECT_ID=aiduxcare-mvp-uat
VITE_FIREBASE_API_KEY=your-uat-api-key
VITE_FIREBASE_AUTH_DOMAIN=aiduxcare-mvp-uat.firebaseapp.com
VITE_FIREBASE_STORAGE_BUCKET=aiduxcare-mvp-uat.appspot.com
VITE_FIREBASE_MESSAGING_SENDER_ID=your-uat-sender-id
VITE_FIREBASE_APP_ID=your-uat-app-id
VITE_FIREBASE_MEASUREMENT_ID=your-uat-measurement-id

# ========================================
# ENTORNO DE PRODUCCIÓN (PROD)
# ========================================
# Copiar a .env.local para producción
# VITE_FIREBASE_PROJECT_ID=aiduxcare-mvp-prod
# VITE_FIREBASE_API_KEY=your-prod-api-key
# VITE_FIREBASE_AUTH_DOMAIN=aiduxcare-mvp-prod.firebaseapp.com
# VITE_FIREBASE_STORAGE_BUCKET=aiduxcare-mvp-prod.appspot.com
# VITE_FIREBASE_MESSAGING_SENDER_ID=your-prod-sender-id
# VITE_FIREBASE_APP_ID=your-prod-app-id
# VITE_FIREBASE_MEASUREMENT_ID=your-prod-measurement-id

# ========================================
# CONFIGURACIÓN GENERAL
# ========================================
VITE_APP_NAME=AiDuxCare V.2
VITE_APP_VERSION=2.0.0
VITE_APP_ENV=development

# ========================================
# ANALYTICS Y MONITOREO
# ========================================
VITE_ANALYTICS_ENABLED=true
VITE_ANALYTICS_PROJECT_ID=your-analytics-project-id
VITE_ANALYTICS_API_KEY=your-analytics-api-key

# ========================================
# SERVICIOS EXTERNOS
# ========================================
VITE_GOOGLE_CLOUD_PROJECT_ID=your-google-cloud-project
VITE_GOOGLE_CLOUD_API_KEY=your-google-cloud-api-key

# ========================================
# CONFIGURACIÓN DE DESARROLLO
# ========================================
VITE_DEV_MODE=true
VITE_DEBUG_ENABLED=true
VITE_MOCK_SERVICES=false

# ========================================
# CONFIGURACIÓN DE TESTING
# ========================================
VITE_TEST_MODE=false
VITE_TEST_FIREBASE_PROJECT_ID=aiduxcare-mvp-test
